{
  "author": "Martin Nabhan",
  "dependencies": {
    "@types/async-retry": "^1.4.2",
    "@types/node-fetch": "^2.5.8",
    "async-retry": "^1.3.1",
    "aws-sdk": "^2.828.0",
    "date-fns": "^2.16.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "serverless": "^2.18.0",
    "serverless-jetpack": "^0.10.7",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "dynamodb-admin": "^4.0.0",
    "dynamodb-local": "0.0.31",
    "run-func": "^1.0.5",
    "s3rver": "^3.6.1",
    "tsc-watch": "^4.2.9"
  },
  "description": "１日単位で見れるHacker News",
  "license": "MIT",
  "name": "hacker-mainichi-api",
  "scripts": {
    "build": "rm -rf ./dist/* && tsc",
    "dev": "NODE_ENV=development NODE_OPTIONS=--enable-source-maps TZ=Asia/Tokyo tsc-watch --noClear --onSuccess \"run-func ./dist/handlers/$function.js handler\"",
    "dynamodb": "concurrently dynamodb-admin dynamodb-local",
    "fix": "prettier --write . && tsc --noEmit",
    "s3": "s3rver --directory ./s3/ --configure-bucket hacker-mainichi ./s3/cors.xml"
  },
  "version": "1.0.0"
}
